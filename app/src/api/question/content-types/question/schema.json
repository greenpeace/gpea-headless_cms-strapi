{
  "kind": "collectionType",
  "collectionName": "questions",
  "info": {
    "singularName": "question",
    "pluralName": "questions",
    "displayName": "Question"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "market": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::market.market",
      "inversedBy": "questions"
    },
    "purpose": {
      "type": "string"
    },
    "thoughts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::thought.thought",
      "mappedBy": "question"
    },
    "responses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::response.response",
      "mappedBy": "question"
    }
  }
}
