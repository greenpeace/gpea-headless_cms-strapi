{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "issue": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::issue.issue",
      "inversedBy": "pages"
    },
    "page_type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::page-type.page-type",
      "inversedBy": "pages"
    },
    "contentHero": {
      "type": "component",
      "repeatable": false,
      "component": "shared.content-hero"
    },
    "thankyouHero": {
      "type": "component",
      "repeatable": false,
      "component": "shared.content-hero"
    },
    "contentBlocks": {
      "type": "dynamiczone",
      "components": [
        "blocks.rich-content"
      ]
    },
    "thankyouBlocks": {
      "type": "dynamiczone",
      "components": [
        "blocks.rich-content"
      ]
    },
    "petitionCta": {
      "type": "string",
      "default": "立即聯署",
      "required": true
    },
    "fixedCta": {
      "type": "string",
      "required": true,
      "default": "立即聯署"
    },
    "fixedThankyouCta": {
      "type": "string",
      "default": "立即捐款",
      "required": true
    },
    "market": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::market.market",
      "inversedBy": "pages"
    },
    "campaign": {
      "type": "string",
      "required": true
    },
    "pageId": {
      "type": "string",
      "private": true
    },
    "campaignId": {
      "type": "string"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "metaSocial": {
      "type": "component",
      "repeatable": false,
      "component": "shared.meta-social"
    },
    "donationModuleCampaign": {
      "type": "string",
      "required": true,
      "default": "general"
    },
    "donationModuleEnv": {
      "type": "enumeration",
      "enum": [
        "test",
        "full",
        "production"
      ],
      "default": "production",
      "required": true
    },
    "donationModuleLanguage": {
      "type": "enumeration",
      "enum": [
        "zh_HK",
        "zh_TW",
        "en_HK"
      ],
      "required": true
    }
  }
}
